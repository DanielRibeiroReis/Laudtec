<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Painel">

    <Button
        android:id="@+id/button5"
        style="@style/botaoPainel"
        android:layout_width="162dp"
        android:layout_height="96dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:drawableTop="@drawable/ic_laudo"
        android:text="Laudo preliminar&#10;(xx)"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/button4"
        style="@style/botaoPainel"
        android:layout_width="162dp"
        android:layout_height="96dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:drawableTop="@drawable/ic_normas"
        android:text="Normas&#10;(xx)"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/painelEdificacoessid" />

    <Button
        android:id="@+id/painelEdificacoessid"
        style="@style/botaoPainel"
        android:layout_width="162dp"
        android:layout_height="96dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="74dp"
        android:drawableStart="@drawable/ic_clientes"


        android:text="&#10;&#10;&#10;Edificações&#10;(xx)"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/aaaa" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:scaleX=".8"
        android:scaleY=".8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logoc" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:text="Olá, usuário"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Serviços Tecnicos"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/button"
        style="@style/botaoPainel"
        android:layout_width="162dp"
        android:layout_height="96dp"
        android:layout_marginEnd="32dp"
        android:drawableTop="@drawable/ic_equipamentos"
        android:text="Equipamentos&#10;(xx)"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/painelEdificacoessid" />

    <Button
        android:id="@+id/button2"
        style="@style/botaoPainel"
        android:layout_width="162dp"
        android:layout_height="96dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:drawableTop="@drawable/ic_questionarios"
        android:text="Questinários&#10;(xx)"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/painelEdificacoessid" />

    <Button
        android:id="@+id/button6"
        style="@style/botaoPainel"
        android:layout_width="162dp"
        android:layout_height="96dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:drawableTop="@drawable/ic_laudo"
        android:text="Laudo final&#10;(xx)"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />

</androidx.constraintlayout.widget.ConstraintLayout>