<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/aaaa"
    tools:context=".painelAdm">


    <TextView
        android:id="@+id/labelGestaoColab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Serviços Gerenciais"
        app:layout_constraintStart_toStartOf="@+id/gestaoColab"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:text="Olá, usuário"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gestaoColab" />

    <TextView
        android:id="@+id/idNumeroLaudosPreliminaresPainel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:elevation="30dp"
        android:text="()"
        android:textColor="@color/cinzaLabel"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelLaudoPreliminar"
        app:layout_constraintTop_toBottomOf="@+id/imageView10" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:elevation="30dp"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelLaudoPreliminar"
        app:layout_constraintTop_toTopOf="@+id/botaoPainelLaudoPreliminar"
        app:srcCompat="@drawable/ic_laudo" />

    <TextView
        android:id="@+id/idNumeroNormasPainel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:elevation="30dp"
        android:text="()"
        android:textColor="@color/cinzaLabel"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelNormas"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <TextView
        android:id="@+id/idNumeroEdificacoesPainel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:elevation="30dp"
        android:text="()"
        android:textColor="@color/cinzaLabel"
        app:layout_constraintStart_toStartOf="@+id/painelEdificacoessid"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <Button
        android:id="@+id/botaoPainelQuestionarios"
        style="@style/botaoPainel"
        android:layout_width="155dp"
        android:layout_height="100dp"
        android:layout_marginTop="8dp"
        android:text="Questionários"
        app:layout_constraintStart_toStartOf="@+id/painelEdificacoessid"
        app:layout_constraintTop_toBottomOf="@+id/painelEdificacoessid" />

    <Button
        android:id="@+id/botaoPainelLaudoPreliminar"
        style="@style/botaoPainel"
        android:layout_width="155dp"
        android:layout_height="100dp"
        android:layout_marginTop="8dp"
        android:text="Laudo preliminar"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelQuestionarios"
        app:layout_constraintTop_toBottomOf="@+id/botaoPainelQuestionarios" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:elevation="30dp"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelQuestionarios"
        app:layout_constraintTop_toTopOf="@+id/botaoPainelQuestionarios"
        app:srcCompat="@drawable/ic_questionarios" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:elevation="30dp"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelNormas"
        app:layout_constraintTop_toTopOf="@+id/botaoPainelNormas"
        app:srcCompat="@drawable/ic_normas" />

    <Button
        android:id="@+id/painelEdificacoessid"
        style="@style/botaoPainel"
        android:layout_width="155dp"
        android:layout_height="100dp"
        android:layout_marginTop="4dp"
        android:enabled="true"
        android:text="Edificações"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:elevation="30dp"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelLaudoFinal"
        app:layout_constraintTop_toTopOf="@+id/botaoPainelLaudoFinal"
        app:srcCompat="@drawable/ic_laudo" />

    <TextView
        android:id="@+id/idNumeroQuestionariosPainel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:elevation="30dp"
        android:text="()"
        android:textColor="@color/cinzaLabel"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelQuestionarios"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/idNumeroEquipamentosPainel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:elevation="30dp"
        android:text="()"
        android:textColor="@color/cinzaLabel"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <Button
        android:id="@+id/botaoPainelLaudoFinal"
        style="@style/botaoPainel"
        android:layout_width="155dp"
        android:layout_height="100dp"
        android:layout_marginTop="8dp"
        android:text="Laudo Final"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelNormas"
        app:layout_constraintTop_toBottomOf="@+id/botaoPainelNormas" />

    <Button
        android:id="@+id/botaoPainelNormas"
        style="@style/botaoPainel"
        android:layout_width="155dp"
        android:layout_height="100dp"
        android:layout_marginTop="8dp"
        android:text="Normas"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelEquipamentos"
        app:layout_constraintTop_toBottomOf="@+id/botaoPainelEquipamentos" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:elevation="30dp"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelEquipamentos"
        app:layout_constraintTop_toTopOf="@+id/botaoPainelEquipamentos"
        app:srcCompat="@drawable/ic_equipamentos" />

    <Button
        android:id="@+id/botaoPainelEquipamentos"
        style="@style/botaoPainel"
        android:layout_width="155dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:text="Equipamentos"
        app:layout_constraintStart_toEndOf="@+id/painelEdificacoessid"
        app:layout_constraintTop_toTopOf="@+id/painelEdificacoessid" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:elevation="30dp"
        app:layout_constraintStart_toStartOf="@+id/painelEdificacoessid"
        app:layout_constraintTop_toTopOf="@+id/painelEdificacoessid"
        app:srcCompat="@drawable/ic_clientes" />

    <TextView
        android:id="@+id/idNumeroLaudoFinalPainel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:elevation="30dp"
        android:text="()"
        android:textColor="@color/cinzaLabel"
        app:layout_constraintStart_toStartOf="@+id/botaoPainelLaudoFinal"
        app:layout_constraintTop_toBottomOf="@+id/imageView11" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Serviços Tecnicos"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="112dp"
        android:layout_marginTop="32dp"
        android:scaleX="0.7"
        android:scaleY="0.7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo1" />

    <Button
        android:id="@+id/gestaoColab"
        style="@style/botaoPainel"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:background="@drawable/botao_painel"
        android:drawableStart="@drawable/ic_colaboradores"
        android:drawablePadding="8dp"
        android:text="Gestão de Colaboradores"
        app:layout_constraintEnd_toEndOf="@+id/botaoPainelEquipamentos"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/painelEdificacoessid"
        app:layout_constraintTop_toBottomOf="@+id/labelGestaoColab"
        tools:visibility="visible" />


</androidx.constraintlayout.widget.ConstraintLayout>