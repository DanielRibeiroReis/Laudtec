<?xml version="1.0" encoding="utf-8"?>
<resources>

        <style name="botaoPricipal" parent="@android:style/Widget.Button">
            <item name="android:gravity">center_vertical|center_horizontal</item>
            <item name="android:textColor">#FFFFFFFF</item>
            <item name="android:textSize">16dip</item>
            <item name="android:textStyle">bold</item>
            <item name="android:background">@drawable/botao_primario</item>
            <item name="android:focusable">true</item>
            <item name="android:clickable">true</item>
        </style>

    <style name="botaooutline" parent="@android:style/Widget.Button">
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:textColor">#CEAA5B</item>
        <item name="android:textSize">16dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/botaooutline</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="inputCustom" parent="@android:style/Widget.EditText">
       <item name="android:textAlignment">viewStart</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">16dip</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/botaooutline</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="botaoPainel" parent="@android:style/Widget.EditText">
        <item name="android:textAlignment">viewStart</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
        <item name="android:textColor">#313131</item>
        <item name="android:textSize">16dip</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/botao_painel</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>






</resources>